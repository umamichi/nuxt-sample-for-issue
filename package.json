    "@types/animejs": "^2.0.2",
    "@types/babel-core": "6.25.6",
    "@types/eslint": "^4.16.6",
    "@types/eslint-plugin-prettier": "2.2.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.15",
